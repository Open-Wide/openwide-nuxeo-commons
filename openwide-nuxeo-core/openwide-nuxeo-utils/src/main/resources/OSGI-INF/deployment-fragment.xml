<?xml version="1.0" ?>
<fragment version="1">

  <extension target="application#MODULE">
    <!-- Module registration -->
    <module>
      <ejb>${bundle.fileName}</ejb>
    </module>
  </extension>
  
  <install>
    <!-- Deploiement des fichiers web -->
    <unzip from="${bundle.fileName}" to="/" prefix="web">
      <include>web/nuxeo.war/**</include>
    </unzip>
  </install>
  
</fragment>