<?xml version="1.0"?>
<component name="fr.openwide.nuxeo.commons.configurablewall.gadgets.contrib" version="1.0">

  <extension target="org.nuxeo.opensocial.gadgets.service" point="gadget">
    <internalGadget name="myworkspacesactivitystream" disabled="false">
      <mountPoint>/myworkspacesactivitystream</mountPoint>
      <directory>configurablewall</directory>
      <entryPoint>configurablewall.xml</entryPoint>
      <category>Open Wide</category>
      <icon>gadget-wall.png</icon>
    </internalGadget>
  </extension>
  
  <documentation>
  <!-- 
    Example additional gadget using the same resources:
    
    <internalGadget name="mywall" disabled="false">
      <mountPoint>/mywall</mountPoint>
      <directory>configurablewall</directory>
      <entryPoint>configurablewall.xml</entryPoint>
      <category>My Company</category>
      <icon>gadget-wall.png</icon>
    </internalGadget>
    
    You'll need to add a translation for key "label.gadget.mywall" for the gadget title.
   -->
   </documentation>
    
</component>
    